<div class="modal fade validation-modal" id="validation-modal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <form method="post" class="" onsubmit="bilan.printBilan(event);">
                <div class="modal-header">
                    <h5 class="modal-title">Confirmer le taux d'imposition</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12">
                            <h5 class="font-size-14 mt-2">
                                <i class="mdi mdi-arrow-right text-primary me-1"></i>Taux appliqué(%)</h5>
                            
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="mb-3 position-relative">
                                        <!--label class="form-label font-size-13 text-muted" for="taux-impot">Taux appliqué(%)</label-->
                                        <input type="number" min="0" step="0.01" class="form-control" id="taux-impot" name="taux-impot" placeholder="Taux d'imposition appliqué" value="5.62" required>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer d-flex justify-content-center">
                    <button type="reset" class="btn btn-light btn-sm" data-bs-dismiss="modal">Annuler</button>
                    <button type="submit" class="btn btn-primary">Confirmer</button>
                </div>
            </form>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>